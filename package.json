{
  "name": "emitterly",
  "version": "1.0.6",
  "description": "Create triggers from file streams",
  "main": "emitterly.js",
  "scripts": {
    "benchmark": "node tests/benchmark/benchmark.js",
    "test": "npx nyc mocha tests/mocha/**/*.js --slow 1 && nyc report --reporter=html",
    "coverage": "npx nyc check-coverage"
  },
  "bin": {
    "emitterly": "./bin/index.js"
  },
  "keywords": [
    "log",
    "event",
    "trigger",
    "stream",
    "tail",
    "watch",
    "action"
  ],
  "bugs": {
    "url": "https://github.com/michaeldegroot/Emitterly/issues",
    "email": "specamps@gmail.com"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "nyc": {
    "branches": 5,
    "lines": 5,
    "functions": 5,
    "statements": 5,
    "exclude": [
      "tests/mocha"
    ]
  },
  "author": {
    "name": "GiveMeAllYourCats",
    "email": "specamps@gmail.com"
  },
  "license": {
    "type": "MIT",
    "url": "https://opensource.org/licenses/MIT"
  },
  "engines": {
    "node": ">=11.0.0 <12.0.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "debug": "^4.1.1",
    "grok-js": "^3.2.3",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "tail": "^2.0.3"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "coveralls": "^3.0.3",
    "husky": "^2.2.0",
    "jsdoc": "^3.6.1",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha-logger": "^1.0.6",
    "moment": "^2.24.0",
    "nyc": "^14.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michaeldegroot/Emitterly.git"
  }
}
